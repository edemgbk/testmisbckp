extends /_layout/email.pug

block view
  .card.c-email-app
    .card-body
      .toolbar.mb-4
        .btn-group
          button.btn.btn-light(type='button')
            svg.c-icon
              use(xlink:href="node_modules/@coreui/icons/sprites/free.svg#cil-envelope-closed")
          button.btn.btn-light(type='button')
            svg.c-icon
              use(xlink:href="node_modules/@coreui/icons/sprites/free.svg#cil-star")
          button.btn.btn-light(type='button')
            svg.c-icon
              use(xlink:href="node_modules/@coreui/icons/sprites/free.svg#cil-bookmark")
        .btn-group
          button.btn.btn-light(type='button')
            svg.c-icon
              use(xlink:href="node_modules/@coreui/icons/sprites/free.svg#cil-share")
          button.btn.btn-light(type='button')
            svg.c-icon
              use(xlink:href="node_modules/@coreui/icons/sprites/free.svg#cil-share-all")
          button.btn.btn-light(type='button')
            svg.c-icon
              use(xlink:href="node_modules/@coreui/icons/sprites/free.svg#cil-share-boxed")
        button.btn.btn-light(type='button')
          svg.c-icon
            use(xlink:href="node_modules/@coreui/icons/sprites/free.svg#cil-trash")
        .btn-group
          button.btn.btn-light.dropdown-toggle(type='button', data-toggle='dropdown')
            svg.c-icon
              use(xlink:href="node_modules/@coreui/icons/sprites/free.svg#cil-tags")
            span.caret
          .dropdown-menu
            a.dropdown-item(href='#')
              | add label
              span.badge.badge-danger  Home
            a.dropdown-item(href='#')
              | add label
              span.badge.badge-info  Job
            a.dropdown-item(href='#')
              | add label
              span.badge.badge-success  Clients
            a.dropdown-item(href='#')
              | add label
              span.badge.badge-warning  News
        .btn-group.float-right
          button.btn.btn-light(type='button')
            svg.c-icon
              use(xlink:href="node_modules/@coreui/icons/sprites/free.svg#cil-chevron-left")
          button.btn.btn-light(type='button')
            svg.c-icon
              use(xlink:href="node_modules/@coreui/icons/sprites/free.svg#cil-chevron-right")
